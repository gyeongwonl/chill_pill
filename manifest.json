{
    "manifest_version": 2,
    "name": "Chill Pill",
    "version": "1.0.0",
    "description": "Chrome extension to help you stay focused on chilling",
    "icons": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    },
    "web_accessible_resources": [
      "css/blocked.css"
    ],
    "background": {
      "scripts": [
        "background.js"
      ]
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": ["tabs", "alarms", "storage"]
  }
  